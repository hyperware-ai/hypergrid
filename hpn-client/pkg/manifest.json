[
  {
    "process_name": "hpnclient",
    "process_wasm_path": "/hpnclient.wasm",
    "on_exit": "Restart",
    "request_networking": true,
    "request_capabilities": [
      "http-server:distro:sys",
      "vfs:distro:sys",
      "eth:distro:sys",
      "homepage:homepage:sys",
      "sqlite:distro:sys",
      "timer:distro:sys"
    ],
    "grant_capabilities": ["http-server:distro:sys", "terminal:terminal:sys"],
    "public": true
  }
]
